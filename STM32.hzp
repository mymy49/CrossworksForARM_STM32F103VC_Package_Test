      project_directory=""
      project_type="Combining" />
    <folder Name="packages">
      <file file_name="packages/STM32_V3_package.xml" />
    </folder>
    <folder
      Name="targets"
      path="targets"
      filter=""
      recurse="Yes"
      exclude="" />
  </project>
</solution>
