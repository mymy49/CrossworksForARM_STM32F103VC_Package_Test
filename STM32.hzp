<!DOCTYPE CrossStudio_Project_File>
<solution Name="STM32" target="8" version="2">
  <project Name="STM32 CPU Support">
    <configuration
      Name="Common"
      combine_command="$(StudioDir)/bin/mkpkg -exclude ./Convert.sh -exclude ./Extract.sh -exclude ./STM32.hzp -exclude ./STM32.hzs -exclude ./STM32.hzq -exclude ./readme.md -exclude ./.gitignore -c &quot;$(CombiningOutputFilePath)&quot; ."
